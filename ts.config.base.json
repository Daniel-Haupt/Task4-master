  "compilerOptions": {
    "moduleResolution": "node16",              
    "module": "esnext",
    "customConditions": ["react-native"],      
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true
  }
}
